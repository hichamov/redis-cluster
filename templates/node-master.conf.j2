bind {{ hostvars[inventory_hostname]['ansible_default_ipv4']['address'] }}
port {{ redis_master_port }}
cluster-enabled yes
cluster-config-file {{ redis_master_run_dir }}/cluster-node-1.conf
cluster-node-timeout 15000
protected-mode no
#requirepass '{{ redis_cluster_password }}'
appendonly yes
appendfilename node-master.aof
dbfilename dump-master.rdb